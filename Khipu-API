import requests

url = "https://payment-api.khipu.com/v3/payments"

payload = {
  "amount": 5000,
  "currency": "CLP",
  "subject": "Cobro de prueba v√≠a API"
}

headers = {
  "Content-Type": "application/json",
  "x-api-key": "XXXXXXX"
}

response = requests.post(url, json=payload, headers=headers)

data = response.json()
print(data)
